{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint",
    "start:dev": "cross-env NODE_ENV=development tsx --watch src/index.ts",
    "start:production": "cross-env NODE_ENV=production tsx src/index.ts",
    "test:unit": "cross-env NODE_ENV=test vitest --run",
    "test:unit:coverage": "cross-env NODE_ENV=test vitest run --coverage",
    "test:unit:watch": "cross-env NODE_ENV=test vitest --reporter=verbose",
    "test:integration": "cross-env NODE_ENV=test vitest --run --config vitest.integration.config.ts --reporter=verbose",
    "test:integration:coverage": "cross-env NODE_ENV=test vitest run --coverage --config vitest.integration.config.ts --reporter=verbose",
    "test:integration:watch": "cross-env NODE_ENV=test vitest --config vitest.integration.config.ts --reporter=verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
