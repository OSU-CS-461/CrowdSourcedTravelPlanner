    "pg": "^8.16.3",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0",
    "zod": "^4.1.12"
  }
